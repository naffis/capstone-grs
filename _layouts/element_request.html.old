---
title: "test"
layout: default
---  
{% include navbar.html offset="0" %}
<div id="api" class="home">		
	{% assign url = page.url | remove: "/api/" | remove: ".html" %}
	{% assign url_components = url | split: "/" %}
	{% assign data_file_name = url_components[0] | downcase %}

	{% assign api_requested_column = url_components[1] | downcase %}
  {% assign api_requested_element = url_components[2] | downcase %}

	<table class="table table-striped table-condensed">
	<thead>
		<tr>				
			{% for header in site.data[data_file_name].first %}
				{% assign sanitized_header = header[0] | sanitize_string %}		
				{% if sanitized_header == api_requested_column %}
					{% assign normal_header = header[0] %}
				{% endif %}
				<th><a href="/api/{{ data_file_name }}/{{ header[0] | sanitize_string }}.html">{{ header[0] }}</a></th>
			{% endfor %}	
		</tr>
	</thead>
	<tbody>			
		{% for row in site.data[data_file_name] %}
			{% assign sanitized_element = row[normal_header] | sanitize_string %}		
			{% if sanitized_element == api_requested_element %}
				<tr>
					{% for element in row %}
						<td>{{ element[1] }}</td>						
					{% endfor %}	
				</tr>
			{% endif %}
		{% endfor %}	
	</tbody>
	</table>

</div>

		
